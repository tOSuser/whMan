;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns1.vpnhome.int. root.siteb.int. (
                             150         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@            IN      NS      ns1.vpnhome.int.
@            IN      NS      ns2.vpnhome.int.
@            IN      A       172.31.0.40
www          IN      A       172.31.0.40
